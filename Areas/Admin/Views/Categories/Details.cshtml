@model Category
@{
    ViewData["Title"] = "Details";
}


<br />
<h2 class="text-info">Details Category</h2>
<br />

    <div class="whiteBackground border">
        

        <div class="form-group row">

            <div class="col-2">
                <label asp-for="Id" class="col-form-label">  </label>
            </div>

            <div class="col-5">
                <input asp-for="Id" readonly class="form-control" />
             
            </div>

        </div>

        <div class="form-group row">

            <div class="col-2">
                <label asp-for="Name" class="col-form-label">  </label>
            </div>

            <div class="col-5">
                <input asp-for="Name" readonly class="form-control" />
                
            </div>

        </div>

        <div class="form-group row">
            <div class="col-5 offset-2">
                <partial name="_EditAndBackToListButtonPartial" model="@Model.Id" />
            </div>
        </div>

    </div>



